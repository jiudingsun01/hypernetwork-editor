    "from functools import partial\n",
    "\n",
    "\n",
    "def disable_dropout(model: torch.nn.Module):\n",
    "    \"\"\"Disable dropout in a model.\"\"\"\n",
    "    for module in model.modules():\n",
    "        if isinstance(module, torch.nn.Dropout):\n",
